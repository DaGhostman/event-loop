{
  "name": "onion/event-loop",
  "license": "MIT",
  "autoload": {
    "psr-4": {
      "Onion\\Framework\\": "src/"
    },
    "files": [
      "src/functions.php",
      "src/bootstrap.php"
    ]
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  },
  "require": {
    "onion/interfaces": "dev-master",
    "psr/event-dispatcher": "^1.0"
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "infection/infection": "^0.25.4",
    "vimeo/psalm": "^4.15",
    "phpspec/prophecy-phpunit": "^2.0"
  },
  "config": {
    "allow-plugins": {
      "infection/extension-installer": true
    }
  }
}
